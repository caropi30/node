{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "outDir": "./build_TS_files",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true, // Deshabilita las comprobaciones de tipos en las declaraciones de biblioteca, lo que puede acelerar la compilaci√≥n.
  },
  "include": [
    "./**/*.ts",
    "./*.mts"
  ]
}
